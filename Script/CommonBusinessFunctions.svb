Public Function CreateHealthCheckDict()

  Dim strLine
  Set fso = CreateObject("Scripting.FileSystemObject")
  Set objFileOpen = fso.OpenTextFile(GetHealthCheckDictionaryPath)
  Set CreateHealthCheckDictObj = CreateObject("Scripting.Dictionary")
  Do Until objFileOpen.AtEndOfStream
  
    strLine =  objFileOpen.ReadLine
    arrLine = Split(strLine,":",2)
    If Not CreateHealthCheckDictObj.Exists(arrLine(0)) Then
      CreateHealthCheckDictObj.Add arrLine(0),arrLine(1)
      ReDim arrLine(0),arrLine(1)
    End If
    
  Loop

  objFileOpen.Close
  Set fso = Nothing
  Set objFileOpen = Nothing
  Set CreateHealthCheckDict = CreateHealthCheckDictObj
  
End Function

