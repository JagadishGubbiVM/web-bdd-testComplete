                    WriteToHealthCheckDictionary "G_db_Appl_Service_Booking_existing","FAIL"
                  End If
            ElseIf dbServiceValidationSIT = "PASS" Then
              WriteToHealthCheckDictionary "G_db_Appl_Service_Booking_existing","PASS"
            End If
  strTimer = 0
  
