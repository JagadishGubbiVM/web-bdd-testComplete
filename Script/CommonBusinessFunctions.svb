Function CreateServiceTableLogs_UAT()

        Dim Recordset,Connection,ConnString
        Dim GV_QADSN,GV_QAServer,GV_QADBName,GV_HCheckUser,GV_HCheckPwd,SQL
        
        Set fso = CreateObject("Scripting.FileSystemObject")
        Set MyFile = fso.CreateTextFile(GetHealthCheckFolderPath&"\DB_Services_UAT.txt", True)
        MyFile.Close

        Set fso = CreateObject("Scripting.FileSystemObject")     
        Set MyFile = fso.OpenTextFile(GetHealthCheckFolderPath&"\DB_Services_UAT.txt",8)
          
        GV_QADSN = "QAAutomation"
        GV_QAServer = "LOW-UATZENSQL1"
        GV_QADBName = "ZenOAO_UAT_9_5"
        GV_HCheckUser = "SVCQAOAO"
        GV_HCheckPwd = "Serqa123"    


    
        ConnString = "Driver=SQL Driver; Data Source=" & GV_QADSN & ";Server=" & GV_QAServer &";Database=" & GV_QADBName & ";Uid= " & GV_HCheckUser & ";Pwd= " & GV_HCheckPwd & ";"
        Set Connection = CreateObject("ADODB.Connection")
        Set Recordset = CreateObject("ADODB.Recordset")
        Connection.Open ConnString
        
            SQL = "SELECT [GlobalPropertyId] AS 'ID'"&_
          " ,[PropertyName] AS 'Name'"&_
          " ,[PropertyValue] AS 'Value'"&_
          " ,[System]"&_
          " FROM [dbo].[GlobalProperty]"&_
          " WHERE [PropertyType] = 'STUB_PARAMS'"&_
          " and [System] in ('CHIMERA','LEXISNEXIS','RFC')"&_
          " ORDER BY [System],[PropertyType],[PropertyName]"
                                          
        Recordset.Open SQL,Connection
          CounterForRow = 0
          Myfile.Writeline("<div class='table-wrap-services'>")
          Myfile.Writeline("<table class='services'>")
          Myfile.Writeline("<tr>")
          Myfile.Writeline("<th>#</th>")
          Myfile.Writeline("<th>ID</th>")
          Myfile.Writeline("<th>Name</th>")
          Myfile.Writeline("<th>Value</th>")
          Myfile.Writeline("<th>System</th>")
          Myfile.Writeline("</tr>")
          
        
          If Recordset.EOF Then
            Log.Message "No Records found for UAT service table"
            
          Else
            Do While NOT Recordset.Eof

              CounterForRow = CounterForRow+1

              Myfile.Writeline("<tr>")
             Myfile.Writeline("<td>"&CounterForRow&"</td>")
              Myfile.Writeline("<td>"&Recordset("ID")&"</td>")
              Myfile.Writeline("<td>"&Recordset("Name")&"</td>")
              Myfile.Writeline("<td>"&Recordset("Value")&"</td>")
             Myfile.Writeline("<td>"&Recordset("System")&"</td>")
              Myfile.Writeline("</tr>")              

              Recordset.MoveNext    
            Loop
            Myfile.Writeline("</table>")
            Myfile.Writeline("</div>")
         End If 
          
    MyFile.Close
    Set fso = Nothing
    Set Connection = Nothing
    Set Recordset = Nothing
End Function
