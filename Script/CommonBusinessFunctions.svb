Public Function DBMailVerification()

  If Ucase(GetEnvironment)= "SIT" Then
         strTimer = strTimer + 1000
         Delay 1000
            If DbMailValidationConnection = "FAIL" Then
                  If strTimer <= 120000 Then 'Waiting for 2 min
                      Call DBMailVerification()
                  Else
                    WriteToHealthCheckDictionary "strOnboardEmail","FAIL"
                  End If
            ElseIf DbMailValidationConnection = "PASS" Then
              WriteToHealthCheckDictionary "strOnboardEmail","PASS"
            End If
  End If
  strTimer = 0
  
End Function

Public Function DbMailValidationConnection()

    Dim GV_QADSN,GV_QAServer,GV_QADBName,GV_HCheckUser,GV_HCheckPwd,ConnString
    
    GV_QADSN = "QAAutomation"
    GV_QAServer = "BPT-SITZENSQL1"
    GV_QADBName = "ZenOAO_SIT_9_5"
    GV_HCheckUser = "SVCQAOAO"
    GV_HCheckPwd = "Serqa123" 
           
    ConnString = "Driver=SQL Driver; Data Source=" & GV_QADSN & ";Server=" & GV_QAServer &";Database=" & GV_QADBName & ";Uid= " & GV_HCheckUser & ";Pwd= " & GV_HCheckPwd & ";"
    Set Connection = CreateObject("ADODB.Connection")
    Set Recordset = CreateObject("ADODB.Recordset")
    Connection.Open ConnString
              SQL = "SELECT ApplicationId, CreatedDate, LogState FROM dbo.AlertLog WHERE  ApplicationID = ('"&Project.Variables.sitAppId&"')"
              Recordset.Open SQL,Connection
                If Recordset.EOF Then
                  Log.Message "No Records found in recordset"
                  DbMailValidationConnection = "FAIL"
                Else
                  Do While NOT Recordset.Eof 
                    If trim(Recordset("LogState")) = "Transmitted" Then
                      DbMailValidationConnection = "PASS"
                      exit do
                    Else
                      DbMailValidationConnection = "FAIL"                     
                    End If
                    Recordset.MoveNext
                  Loop
              End If
              
  Set Connection = Nothing
  Set Recordset = Nothing


End Function

