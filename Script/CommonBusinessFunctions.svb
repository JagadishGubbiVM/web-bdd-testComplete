Function SendEmailFromOutlook()

  Dim sitPassString,sitFailString,sitNoRunString,uatPassString,uatFailString,uatNoRunString,sMsgBody

        Set objOutlook = CreateObject("Outlook.Application")
        Set myMail = objOutlook.CreateItem(0)
        myMail.To="satish.ravula@valuemomentum.com;"
       ' myMail.To = "Balamurugan.Arjunan@peoples.com;David.Hacker@peoples.com;Lee.Vardaro@peoples.com;Stephen.Adcock@peoples.com;Regina.Trombly@peoples.com;Craig.Novick@peoples.com;Alexandra.Kamyk@peoples.com"
       ' myMail.Cc="Pasupula.Saleem@peoples.com;sagar.shintre@peoples.com;Yamini.Mule@peoples.com;Dinesh.Banjan@peoples.com;Vinodh.Somalaraju@peoples.com;Saipooja.Muddivetti@peoples.com"

 
  sitPassString = "<h1>SIT - <font  color='green'>PASS</font></h1>"
  sitFailString = "<h1>SIT - <font  color='red'>FAIL</font></h1>"
  sitNoRunString = "<h1>SIT - <font  color='orange'>NO RUN</font></h1>"
  
  uatPassString = "<h1>UAT - <font  color='green'>PASS</font></h1>"
  uatFailString = "<h1>UAT - <font color='red'>FAIL</font></h1>"
  uatNoRunString = "<h1>UAT - <font  color='orange'>NO RUN</font></h1>"
  


        Set objFileToRead = CreateObject("Scripting.FileSystemObject").OpenTextFile(GetHealthCheckFolderPath&"\SmokeTestLog.html",1)
        sMsgBody =  objFileToRead.ReadAll()
        objFileToRead.Close
        Set objFileToRead = Nothing
  
  If instr(sMsgBody,sitPassString) > 0 Then
    sitStatus = "PASS"
  ElseIf instr(sMsgBody,sitFailString) > 0 Then
    sitStatus = "FAIL"
  ElseIf instr(sMsgBody,sitNoRunString) > 0 Then
    sitStatus = "NO RUN"
  End If

  If instr(sMsgBody,uatPassString) > 0 Then
    uatStatus = "PASS"
  ElseIf instr(sMsgBody,uatFailString) > 0 Then
    uatStatus = "FAIL"
  ElseIf instr(sMsgBody,uatNoRunString) > 0 Then
    uatStatus = "NO RUN"
  End If 

  myMail.Subject = "Health Check : "& date &" - SIT ("&sitStatus&"), UAT ("&uatStatus&")"
        myMail.HTMLBody  = sMsgBody
        myMail.Send


        Set myMail = Nothing
        Set objOutlook = Nothing
  
  
End Function

