Public Function EvaluateElementByCSS(webPage,css)
On Error Resume Next
Dim arrObj
      arrObj= webPage.QuerySelector(GetLocator(css))
  
     If Not arrObj Is Nothing Then
        EvaluateElementByCSS= True
     Else 
        EvaluateElementByCSS= False
    End If 
 On Error GoTo 0

End Function

